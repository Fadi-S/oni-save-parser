{
  "name": "oni-save-parser",
  "version": "14.0.1",
  "description": "Parser for Oxygen Not Included save files.",
  "main": "lib/index.js",
  "types": "dts/index.d.ts",
  "scripts": {
    "clean": "rimraf lib dts",
    "prebuild": "npm run clean",
    "build": "tsc -p src",
    "watch": "tsc -p src -w",
    "format": "prettier --write ./src/**/*.ts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ONI",
    "Oxygen Not Included",
    "save editor"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/RoboPhred/oni-save-parser.git"
  },
  "author": "RoboPhred",
  "license": "MIT",
  "devDependencies": {
    "@types/deep-diff": "1.0.5",
    "@types/lodash.flowright": "^3.5.9",
    "@types/long": "^3.0.32",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.14.1",
    "@types/pako": "^2.0.3",
    "@types/text-encoding": "0.0.40",
    "deep-diff": "^1.0.2",
    "lodash.flowright": "^3.5.0",
    "minimist": "^1.2.8",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "jsonschema": "^1.5.0",
    "long": "^5.3.2",
    "pako": "^2.1.0",
    "text-encoding": "^0.7.0"
